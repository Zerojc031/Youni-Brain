/* pages/page1/page1.wxss */
.rank{
  font-size: 20px;
  color:rgb(85, 94, 88);
  background-color:rgb(245, 247, 156);
  text-align: center;
  border-radius: 8px;
  width: 100%;
  height: 10%;
  box-shadow: 0 8px 16px 0 rgba(0,0,0,0.2), 0 3px 10px 0 rgba(0,0,0,0.19);
}
.userinfo-avatar {
  width: 80rpx; 
  height: 80rpx;
}
.title{
  color:rgb(63, 160, 184);
  font-size: 30px;
  text-align: center;
  background: none;
  padding: 20px 20px;
}
.block{
  padding:10px 0px;
  background: none;
  outline-color: none;
  border-radius: 0px;
}
.text{
  font-size: 15px;
  color: rgb(180, 175, 175);
  padding: 10px 15px;
}
.ranknum{
  font-size: 30px;
  color: gold;
  font-weight: bold;
  padding:5px 15px;
  float: left;
}
.score{
  font-size: 20px;
  padding:0 20px;
  color: rgb(69, 157, 182);
  float:right;
}
.pre_page{
  float: left;
  width: 50%;
  background: none;
  font-size: 10px;
  color: gray;
  padding: 6px 6px 0px 0px;
  margin: 0px 0px 5px 0px;
}
.pre_page::after{
  border-style: none;
  outline: none; 
}
.pre_page.next_page{
  float: right;
}
.pageChange{
  color: white;
}